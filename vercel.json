{
  "version": 2,
  "installCommand": "cd backend && npm install",
  "buildCommand": "cd backend && npm run build:vercel && npm run db:generate",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"]
}
