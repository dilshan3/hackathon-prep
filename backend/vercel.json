{
  "version": 2,
  "name": "logistics-issue-tracker-api",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/index.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "installCommand": "npm install",
  "buildCommand": "npm run build && npm run db:generate"
}
